{
  "name": "react-app",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/donniean/react-app#readme",
  "bugs": {
    "url": "https://github.com/donniean/react-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/react-app.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "scripts": {
    "build": "pnpm run i18n:types && tsc --build && vite build",
    "dev": "vite",
    "docker:build": "docker buildx build --tag donniean/react-app --push .",
    "docker:build:multi": "docker buildx build --platform linux/amd64,linux/arm64 --tag donniean/react-app --push .",
    "docker:run": "docker run --publish=3000:80 -it --rm donniean/react-app",
    "i18n:check": "pnpm run i18n:status && pnpm run i18n:extract --ci --dry-run && pnpm run i18n:lint",
    "i18n:extract": "i18next-cli extract",
    "i18n:lint": "i18next-cli lint",
    "i18n:status": "i18next-cli status",
    "i18n:types": "i18next-cli types && prettier --write src/@types/resources.d.ts",
    "preinstall": "pnpm dlx only-allow pnpm",
    "knip": "knip",
    "knip:fix": "pnpm run knip --fix",
    "lint": "concurrently --group --timings --prefix-colors=auto \"pnpm:lint:*(!:fix)\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "pnpm run lint:css --fix",
    "lint:fix": "concurrently --max-processes=1 --group --timings --prefix-colors=auto \"pnpm:lint:*:fix\"",
    "lint:format": "prettier --check --ignore-unknown .",
    "lint:format:fix": "prettier --write --ignore-unknown .",
    "lint:html": "htmlhint --ignore=\"**/coverage/**\" \"**/*.html\"",
    "lint:js": "eslint",
    "lint:js:fix": "pnpm run lint:js --fix",
    "lint:md": "markdownlint --dot \"**/*.md\"",
    "lint:md:fix": "pnpm run lint:md --fix",
    "lint:package-json": "pnpm run lint:package-json:fix --check",
    "lint:package-json:fix": "sort-package-json \"**/package.json\" --ignore \"**/node_modules/**/package.json\" --ignore \"**/dist/**/package.json\"",
    "lint:spell": "cspell lint --no-progress --no-must-find-files --dot --gitignore .",
    "lint:text": "autocorrect --lint",
    "lint:text:fix": "autocorrect --fix",
    "lint:types": "tsc --build --clean && tsc --build",
    "ncu": "pnpm dlx npm-check-updates --deep",
    "ncu:upgrade": "pnpm run ncu --upgrade",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@tanstack/react-query": "5.90.9",
    "@tanstack/react-query-devtools": "5.90.2",
    "clsx": "2.1.1",
    "i18next": "25.6.2",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-resources-to-backend": "1.2.1",
    "lodash-es": "4.17.21",
    "qs": "6.14.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "6.0.0",
    "react-i18next": "16.3.3",
    "react-router": "7.9.6",
    "remeda": "2.32.0",
    "tailwind-merge": "3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/compat": "2.0.0",
    "@eslint/js": "9.39.1",
    "@tailwindcss/vite": "4.1.17",
    "@tanstack/eslint-plugin-query": "5.91.2",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.10.1",
    "@types/qs": "6.14.0",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-v8": "4.0.9",
    "@vitest/eslint-plugin": "1.4.2",
    "@vitest/ui": "4.0.9",
    "autocorrect-node": "2.14.0",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "concurrently": "9.2.1",
    "cspell": "9.3.2",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-i18next": "6.1.3",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-unicorn": "62.0.0",
    "eslint-plugin-unused-imports": "4.3.0",
    "globals": "16.5.0",
    "htmlhint": "1.7.1",
    "husky": "9.1.7",
    "i18next-cli": "1.24.9",
    "knip": "5.69.1",
    "lint-staged": "16.2.6",
    "markdownlint-cli": "0.45.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "sort-package-json": "3.4.0",
    "stylelint": "16.25.0",
    "stylelint-config-css-modules": "4.5.1",
    "stylelint-config-recess-order": "7.4.0",
    "stylelint-config-standard": "39.0.1",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "typescript-plugin-css-modules": "5.2.0",
    "vite": "7.2.2",
    "vite-plugin-checker": "0.11.0",
    "vite-plugin-svgr": "4.5.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.9"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  }
}
