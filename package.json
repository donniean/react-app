{
  "name": "react-app",
  "version": "1.0.0",
  "description": "react-app",
  "homepage": "https://github.com/donniean/react-app#readme",
  "bugs": {
    "url": "https://github.com/donniean/react-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/react-app.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "commit": "cz",
    "dev": "vite",
    "docker:build": "docker build --tag=donniean/react-app .",
    "docker:push": "docker push donniean/react-app",
    "docker:run": "docker run --publish=3000:80 -it --rm donniean/react-app",
    "preinstall": "npx only-allow npm",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts,tsx}\"",
    "lint:eslint:fix": "eslint --fix \"**/*.{js,mjs,cjs,ts,tsx}\"",
    "lint:htmlhint": "htmlhint \"**/*.html\"",
    "lint:markdownlint": "markdownlint --dot \"**/*.md\"",
    "lint:markdownlint:fix": "markdownlint --dot --fix \"**/*.md\"",
    "lint:prettier": "prettier --check --ignore-unknown \"**\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:sort-package-json": "sort-package-json --check",
    "lint:sort-package-json:fix": "sort-package-json",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "lint:stylelint:fix": "stylelint --fix \"**/*.css\"",
    "lint:tsc": "tsc --noEmit",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@donniean/styles": "^1.5.0",
    "@tanstack/react-query": "^5.28.14",
    "@tanstack/react-query-devtools": "^5.28.14",
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "lodash": "^4.17.21",
    "qs": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/eslint": "^8.56.7",
    "@types/lodash": "^4.17.0",
    "@types/qs": "^6.9.14",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cspell": "^8.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.25.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-vitest": "^0.4.1",
    "htmlhint": "^1.1.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markdownlint-cli": "^0.39.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "sort-package-json": "^2.10.0",
    "stylelint": "^16.3.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.7",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
