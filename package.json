{
  "name": "react-app",
  "version": "0.1.0",
  "description": "react-app",
  "homepage": "https://github.com/donniean/react-app#readme",
  "bugs": {
    "url": "https://github.com/donniean/react-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/react-app.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "commit": "cz",
    "dev": "vite",
    "preinstall": "npx only-allow npm",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts,tsx}\"",
    "lint:eslint:fix": "eslint --fix \"**/*.{js,mjs,cjs,ts,tsx}\"",
    "lint:htmlhint": "htmlhint \"**/*.html\"",
    "lint:markdownlint": "markdownlint --dot \"**/*.md\"",
    "lint:markdownlint:fix": "markdownlint --dot --fix \"**/*.md\"",
    "lint:prettier": "prettier --check --ignore-unknown \"**\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:sort-package-json": "sort-package-json --check",
    "lint:sort-package-json:fix": "sort-package-json",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "lint:stylelint:fix": "stylelint --fix \"**/*.css\"",
    "lint:tsc": "tsc --noEmit",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@donniean/styles": "^1.5.0",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.29.25",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "lodash": "^4.17.21",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/eslint": "^8.44.0",
    "@types/lodash": "^4.14.195",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cspell": "^6.31.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^48.0.0",
    "eslint-plugin-vitest": "^0.2.6",
    "htmlhint": "^1.1.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "markdownlint-cli": "^0.35.0",
    "postcss": "^8.4.26",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.0",
    "sort-package-json": "^2.5.1",
    "stylelint": "^15.10.2",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
