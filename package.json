{
  "name": "react-app",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/donniean/react-app#readme",
  "bugs": {
    "url": "https://github.com/donniean/react-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/react-app.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "scripts": {
    "build": "npm run i18n:types && tsc && vite build",
    "dev": "vite",
    "docker:build": "docker buildx build --tag donniean/react-app --push .",
    "docker:build:multi": "docker buildx build --platform linux/amd64,linux/arm64 --tag donniean/react-app --push .",
    "docker:run": "docker run --publish=3000:80 -it --rm donniean/react-app",
    "i18n:check": "npm run i18n:extract -- --fail-on-update",
    "i18n:clean": "rimraf src/locales/**/*_old.json",
    "i18n:extract": "i18next",
    "i18n:types": "i18next-resources-for-ts interface -i src/locales/en/ -o src/@types/resources.d.ts && prettier --write src/@types/resources.d.ts",
    "preinstall": "npx only-allow npm",
    "knip": "knip",
    "knip:fix": "npm run knip -- --fix",
    "lint": "concurrently --group --timings --prefix-colors=auto \"npm:lint:*(!:fix)\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "concurrently --max-processes=1 --group --timings --prefix-colors=auto \"npm:lint:*:fix\"",
    "lint:format": "prettier --check --ignore-unknown .",
    "lint:format:fix": "prettier --write --ignore-unknown .",
    "lint:html": "htmlhint --ignore=\"**/coverage/**\" \"**/*.html\"",
    "lint:js": "eslint",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:md": "markdownlint --dot \"**/*.md\"",
    "lint:md:fix": "npm run lint:md -- --fix",
    "lint:package-json": "npm run lint:package-json:fix -- --check",
    "lint:package-json:fix": "sort-package-json \"**/package.json\" --ignore \"**/node_modules/**/package.json\" --ignore \"**/dist/**/package.json\"",
    "lint:spell": "cspell lint --no-progress --no-must-find-files --dot --gitignore .",
    "lint:text": "autocorrect --lint",
    "lint:text:fix": "autocorrect --fix",
    "lint:types": "tsc --noEmit",
    "ncu": "npx npm-check-updates --deep",
    "ncu:upgrade": "npm run ncu -- --upgrade",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "clsx": "^2.1.1",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "lodash-es": "^4.17.21",
    "qs": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.7.2",
    "react-router": "^7.8.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "autocorrect-node": "^2.14.0",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "concurrently": "^9.2.1",
    "cspell": "^9.2.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "htmlhint": "^1.6.3",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "i18next-resources-for-ts": "^1.6.0",
    "knip": "^5.63.0",
    "lint-staged": "^16.1.5",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "sort-package-json": "^3.4.0",
    "stylelint": "^16.23.1",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-recess-order": "^7.2.0",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.1.3",
    "vite-plugin-checker": "^0.10.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  }
}
