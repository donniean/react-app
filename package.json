{
  "name": "react-app",
  "version": "1.0.0",
  "description": "react-app",
  "homepage": "https://github.com/donniean/react-app#readme",
  "bugs": {
    "url": "https://github.com/donniean/react-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/react-app.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "docker:build": "docker build --tag=donniean/react-app .",
    "docker:push": "docker push donniean/react-app",
    "docker:run": "docker run --publish=3000:80 -it --rm donniean/react-app",
    "preinstall": "npx only-allow npm",
    "knip": "knip",
    "knip:fix": "npm run knip -- --fix",
    "lint": "concurrently --group --timings --prefix-colors=auto \"npm:lint:*(!:fix)\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "concurrently --max-processes=1 --group --timings --prefix-colors=auto \"npm:lint:*:fix\"",
    "lint:format": "prettier --check --ignore-unknown \"**\"",
    "lint:format:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:html": "htmlhint --ignore=\"**/coverage/**\" \"**/*.html\"",
    "lint:js": "eslint",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:md": "markdownlint --dot \"**/*.md\"",
    "lint:md:fix": "npm run lint:md -- --fix",
    "lint:package-json": "npm run lint:package-json:fix -- --check",
    "lint:package-json:fix": "sort-package-json",
    "lint:spell": "cspell lint --no-progress --no-must-find-files --gitignore .",
    "lint:text": "autocorrect --lint",
    "lint:text:fix": "autocorrect --fix",
    "lint:types": "tsc --noEmit",
    "ncu": "npx ncu --deep",
    "ncu:upgrade": "npm run ncu -- --upgrade",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-query-devtools": "^5.76.1",
    "clsx": "^2.1.1",
    "lodash-es": "^4.17.21",
    "qs": "^6.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.18",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/eslint-plugin": "^1.1.44",
    "autocorrect-node": "^2.14.0",
    "concurrently": "^9.1.2",
    "cspell": "^9.0.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.11.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "htmlhint": "^1.1.4",
    "husky": "^9.1.7",
    "knip": "^5.56.0",
    "lint-staged": "^16.0.0",
    "markdownlint-cli": "^0.44.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sort-package-json": "^3.2.1",
    "stylelint": "^16.19.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
